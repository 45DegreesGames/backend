# Clave API para Gemini AI
API_KEY=tu_clave_api_de_gemini

# Configuración de IA
AI_PROVIDER=gemini
# Modelos disponibles:
# - gemini-pro (recomendado para general)
# - gemini-1.5-pro (más potente pero requiere ajustes)
# - gemini-1.5-flash-latest (más rápido)
# - gemini-2.0-flash-thinking-exp-01-21 (modelo experimental avanzado)
GEMINI_MODEL=gemini-2.0-flash-thinking-exp-01-21
TIMEOUT_SECONDS=30

# Configuración CORS
ALLOWED_ORIGINS=https://tudominio.com,http://localhost:3000

# Configuración de PDFLatex
FORCE_LATEX_ONLY_MODE=False
FORCE_PDF_GENERATION=False
USE_SIMPLE_PDFLATEX=True
TEMP_FILE_TTL=600

# Puerto del servidor
PORT=8000 